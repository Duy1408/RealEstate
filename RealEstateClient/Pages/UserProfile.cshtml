}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>EstateAgency Bootstrap Template - Index</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link href="img/favicon.png" rel="icon">
    <link href="img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="lib/bootstrap/animate.css/animate.min.css" rel="stylesheet">
    <link href="lib/bootstrap/bootstrapindex/css/bootstrap.min.css" rel="stylesheet">
    <link href="lib/bootstrap/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="lib/bootstrap/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="css/style.css" rel="stylesheet">

</head>

<body>

    <!-- ======= Header/Navbar ======= -->
    <nav class="navbar navbar-default navbar-trans navbar-expand-lg fixed-top">
        <div class="container">
            <button class="navbar-toggler collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#navbarDefault" aria-controls="navbarDefault" aria-expanded="false" aria-label="Toggle navigation">
                <span></span>
                <span></span>
                <span></span>
            </button>
            <a class="navbar-brand text-brand" href="index.html">Estate<span class="color-b">Agency</span></a>

            <div class="navbar-collapse collapse justify-content-center" id="navbarDefault">
                <ul class="navbar-nav">

                    <li class="nav-item">
                        <a class="nav-link active" asp-page="/HomePage">Home</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link " asp-page="/UserProfile">About</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link " asp-page="/ShowAllRealEstate">Property</a>
                    </li>
                </ul>
            </div>

        </div>
    </nav><!-- End Header/Navbar -->

    <main id="main">

        <!-- ======= Intro Single ======= -->
        <section class="intro-single">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 col-lg-8">
                        <div class="title-single-box">
                            <h1 class="title-single">Margaret Stone</h1>
                            <span class="color-text-a">Agent Immobiliari</span>
                        </div>
                    </div>
                    <div class="col-md-12 col-lg-4">
                        <nav aria-label="breadcrumb" class="breadcrumb-box d-flex justify-content-lg-end">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item">
                                    <a asp-page="/HomePage">Home</a>
                                </li>
                                <li class="breadcrumb-item">
                                    <a asp-page="/RealEstateDetails" asp-route-id="@Model.User.UserID">My RealEstate</a>
                                </li>
                                <li class="breadcrumb-item">
                                    <a asp-page="/CreateBid">My Bid</a>
                                </li>
                                <li class="breadcrumb-item">
                                    <a asp-page="/CreateRealEstatePage">My Create RealEstate</a>
                                </li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </section><!-- End Intro Single -->
        <!-- ======= Agent Single ======= -->
        <section class="agent-single">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="agent-avatar-box">
                                    <img src="img/pic3.jpg" alt="" class="agent-avatar img-fluid">
                                </div>
                            </div>
                            <div class="col-md-5 section-md-t3">
                                <div class="agent-info-box">
                                    <div class="agent-title">
                                        <div class="title-box-d">
                                            <h3 class="title-d">
                                                @Model.User.UserName
                                                <br> @Model.User.FullName
                                            </h3>
                                        </div>
                                    </div>
                                    <div class="agent-content mb-3">
                                        <div class="info-agents color-a">
                                            <p>
                                                <strong>Phone: </strong>
                                                <span class="color-text-a"> @Model.User.PhoneNumber </span>
                                            </p>
                                            <p>
                                                <strong>Email: </strong>
                                                <span class="color-text-a"> @Model.User.Email</span>
                                            </p>
                                            <p>
                                                <strong>Amount: </strong>
                                                <span class="color-text-a"> @Model.User.BidAmount</span>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section><!-- End Agent Single -->

    </main><!-- End #main -->
    <!-- ======= Footer ======= -->
    <section class="section-footer">
        <div class="container">
            <div class="row">
                <div class="col-sm-12 col-md-4">
